{
    "name": "quillstack/middleware",
    "description": "The middleware library based on PSR-15: HTTP Server Request Handlers.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "middleware",
        "php",
        "psr",
        "psr-15",
        "php8"
    ],
    "homepage": "https://quillstack.org/",
    "authors": [
        {
            "name": "Radek Ziemniewicz",
            "email": "radek@quillstack.org"
        }
    ],
    "require": {
        "php": "^8",
        "psr/http-server-middleware": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/container": "^1.0",
        "quillstack/router": "^0.0.3",
        "quillstack/response": "^0.0.2"
    },
    "autoload": {
        "psr-4": {
            "Quillstack\\Middleware\\": "src/",
            "Quillstack\\Middleware\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "quillstack/unit-tests": "^0.2.15"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}
